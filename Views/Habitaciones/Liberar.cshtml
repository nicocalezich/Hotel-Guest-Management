@model ProyectoHotelNT1.Models.Habitaciones

@{
    ViewData["Title"] = "Liberar";
}

<h2>Liberar habitacion</h2>

<h3>Esta seguro que quiere liberar la habitacion?</h3>
<div>
    <h4>Habitacion <b>Nro. @Html.DisplayFor(model => model.NroHabitacion)</b> </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nro. habitacion
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NroHabitacion)
        </dd>
        <dt>
            Esta ocupada
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EstaOcupada)
        </dd>
        <dt>
            Precio por dia
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PrecioPorDia)
        </dd>
        <dt>
            Capacidad maxima
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CapacidadMax)
        </dd>
    </dl>

    <form asp-action="Liberar">
        <a asp-action="Index">Volver</a> |
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Liberar" class="btn btn-default" />
    </form>
</div>
